# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Lemon Squeezy Configuration
# API Key - Get from: https://app.lemonsqueezy.com/settings/api
VITE_LEMON_SQUEEZY_API_KEY=your_lemon_squeezy_api_key

# Store ID - Get from your Lemon Squeezy store settings
VITE_LEMON_SQUEEZY_STORE_ID=your_store_id

# Product IDs - Get from your Lemon Squeezy products page
VITE_LEMON_PRODUCT_ID_STANDARD=your_standard_product_id
VITE_LEMON_PRODUCT_ID_PREMIUM=your_premium_product_id

# Checkout IDs - Get from "Copy checkout link" in Lemon Squeezy (the ID after /buy/)
VITE_LEMON_CHECKOUT_ID_STANDARD=your_standard_checkout_id
VITE_LEMON_CHECKOUT_ID_PREMIUM=your_premium_checkout_id

# Webhook Secret - Create webhook at: https://app.lemonsqueezy.com/settings/webhooks
# Webhook URL should be: https://your-supabase-project.supabase.co/functions/v1/lemon-squeezy-webhook
LEMON_SQUEEZY_WEBHOOK_SECRET=your_webhook_secret
